{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2022", "DOM"],
    "target": "ES2015",
    "jsx": "react-jsx",
    "checkJs": true,
    "noEmit": false,
    "outDir": "dist",
    "importHelpers": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules",
        "options": { "customMatcher": "\\.css$" }
      }
    ]
  },
  "include": ["src"]
}
